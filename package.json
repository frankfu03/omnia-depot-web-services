{
  "name": "omnia-depot-web-services",
  "version": "0.0.1",
  "description": "A web service implemented in nodejs with TypeORM that provides RESTful apis for searching and order goods from the omnia depot.",
  "main": "server.ts",
  "engines": {
    "node": "20.14.0"
  },
  "scripts": {
    "apidoc": "redocly lint openapi/openapi.yaml && redocly build-docs openapi/openapi.yaml --output=./generated-files/odws-openapi-doc.html",
    "apijson": "redocly lint openapi/openapi.yaml && redocly bundle openapi/openapi.yaml --output=./openapi/openapi.json",
    "clean": "shx rm -rf coverage build, dist, openapi/openapi.json",
    "start": "node dist/server.js",
    "build": "tsc",
    "lint": "eslint",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frankfu03/omnia-depot-web-services.git"
  },
  "keywords": [
    "Express",
    "NodeJS",
    "Online Store",
    "OpenAPI",
    "PostgreSQL",
    "TypeORM",
    "TypeScript",
    "Web Services"
  ],
  "author": "Frank Q. Fu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frankfu03/omnia-depot-web-services/issues"
  },
  "homepage": "https://github.com/frankfu03/omnia-depot-web-services#readme",
  "devDependencies": {
    "@redocly/cli": "^1.27.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.13",
    "@types/nock": "^11.1.0",
    "@types/node": "^22.7.4",
    "jest": "^29.7.0",
    "nock": "^13.5.6",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "express-openapi-validator": "^5.3.9",
    "helmet": "^8.0.0",
    "http-proxy-middleware": "^3.0.3",
    "lodash": "^4.17.21",
    "mini-moment": "^0.1.1",
    "typeorm": "^0.3.20"
  }
}
